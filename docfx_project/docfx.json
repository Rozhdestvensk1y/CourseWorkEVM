{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "**/*.csproj"
          ]
        }
      ],
      "dest": "docs/api",
      "disableGitFeatures": false,
      "disableDefaultFilter": false,
      "globOptions": {
        "ignore": [
          "**/obj/**",
          "**/bin/**"
        ]
      }
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "index.md"
        ]
      }
    ],
    "globalMetadata": {
      "_appTitle": "My Documentation",
      "_disableSearch": false,
      "_disableSidebar": false
    },
    "dest": "_site",
    "fileMetadata": [
      {
        "files": [
          "index.md"
        ],
        "properties": {
          "title": "Home",
          "meta_keywords": "documentation, docfx, example",
          "meta_description": "This is an example documentation generated with DocFX."
        }
      }
    ],
    "template": [
      "default"
    ],
    "postProcessors": [
      "logStatistics",
      "cleanup"
    ],
    "cleanup": [
      {
        "files": [
          "_site/images/exclude/**"
        ]
      }
    ]
  },
  "serve": {
    "hostname": "localhost",
    "port": 8080
  }
}
